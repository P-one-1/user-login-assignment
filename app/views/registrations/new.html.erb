<%= form_with model: @user, url: "/users/create", method: :post do |f| %>
	<div>
	<%= f.label "User Name"%><br>
	<%= f.text_field :username%>
	</div>
	<div>
	<%= f.label "email"%><br>
	<%= f.email_field :email%>
	</div>
	<div>
	<%= f.label "Password"%><br>
	<%= f.password_field :password%>
	</div>
	<div>
	<%= f.label "Password Confirmation"%><br>
	<%= f.password_field :password_confirmation%>
	</div>
	<div>
	<%= f.submit 'Sign-Up'%>
	</div>
<%end%>