<h1>Log In</h1>
<%= form_tag sessions_create_path do %>
	<div>
		<%= label_tag :email %><br>
		<%= text_field_tag :email %>
	</div>
	<div>
		<%= label_tag :password %><br>
		<%= password_field_tag :password %>
	</div>
	<div>
		<%= submit_tag "Log In" %>
	</div>
	<div>
		<%= link_to 'Do not have account?', registrations_new_path%>
	</div>
<% end %>
